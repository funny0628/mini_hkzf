<view class="citylist">
	<view class="top">
		<text class="iconfont icon-fenye-shangyiye"></text>
		<text class="title">城市选择</text>
	</view>
	<view class="state" wx:for="{{allcitylist}}" wx:key="index"  wx:for-item="item1">
		<text class="stateitem">{{item1.title}}</text>
		<view class="city">
			<text  wx:for="{{item1.children}}" wx:key="index" wx:for-item="item2" class="cityitem">{{item2}}</text>
		</view>
	</view>
	<view class="cityfirname">
		<text class="topname" wx:for="{{maoidan}}" wx:key="index">{{item}}</text>
	</view>
</view>